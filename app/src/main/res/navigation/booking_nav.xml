<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/booking_nav"
    app:startDestination="@id/booking_confirmation">

    <fragment
        android:id="@+id/booking_confirmation"
        android:name="com.book.chore.ui.services.booking_confirmation"
        android:label="fragment_booking_confirmation"
        tools:layout="@layout/fragment_booking_confirmation" >
        <action
            android:id="@+id/action_booking_confirmation_to_booking_confirmed"
            app:destination="@id/booking_confirmed" />
    </fragment>
    <fragment
        android:id="@+id/booking_confirmed"
        android:name="com.book.chore.ui.services.booking_confirmed"
        android:label="fragment_booking_confirmed"
        tools:layout="@layout/fragment_booking_confirmed" />
</navigation>